@model IEnumerable<MoneyManagerUI.Models.User>

<h2>List of users</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">User Roles</a>
                <a class="btn btn-sm btn-primary" asp-controller="Roles" asp-action="ChangePassword" asp-route-id="@user.Id">Change Password</a>
            </td>
        </tr>
    }
</table>